{
  "version": "0.1.0",
  "license": "MIT",
  "name": "@zach.codes/react-calendar",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build && npm run build-tailwind",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build && npm run build-tailwind",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build-tailwind": "NODE_ENV=production tailwindcss build src/tailwind.css -o dist/calendar-tailwind.css"
  },
  "peerDependencies": {
    "date-fns": ">=2",
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "Zach Silveira",
  "module": "dist/@zach.codes/react-calendar.esm.js",
  "size-limit": [
    {
      "path": "dist/@zach.codes/react-calendar.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/@zach.codes/react-calendar.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@sinonjs/fake-timers": "^7.0.2",
    "@size-limit/preset-small-lib": "^4.10.1",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/addons": "^6.1.21",
    "@storybook/react": "^6.1.21",
    "@tailwindcss/postcss7-compat": "^2.0.3",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.1",
    "autoprefixer": "^9",
    "babel-loader": "^8.2.2",
    "date-fns": "^2.19.0",
    "husky": "^5.1.3",
    "identity-obj-proxy": "^3.0.0",
    "postcss": "^7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "rollup-plugin-postcss": "^4.0.0",
    "size-limit": "^4.10.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "jest": {
    "setupFiles": [
      "./test/setup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  }
}
